{
  "name": "rx-ui-patterns",
  "version": "1.0.0",
  "description": "Patterns for UI development with RxJS",
  "main": "index.js",
  "scripts": {
    "build": "npm run snowpack",
    "snowpack": "snowpack --clean --optimize --no-source-map --dest ./public/web_modules/",
    "postinstall": "npm run build",
    "start": "es-dev-server --open --watch --app-index ./index.html --root-dir ./public/",
    "test": "npm run test-style && npm run test-coverage",
    "test-style": "standard --fix",
    "test-spec": "mocha --require esm",
    "test-coverage": "nyc --require esm --reporter=text --reporter=text-summary npm run test-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basham/rx-ui-patterns.git"
  },
  "author": "Chris Basham <chris@bash.am>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basham/rx-ui-patterns/issues"
  },
  "homepage": "https://github.com/basham/rx-ui-patterns#readme",
  "devDependencies": {
    "chai": "4.2.0",
    "es-dev-server": "1.48.1",
    "esm": "3.2.25",
    "mocha": "7.1.2",
    "nyc": "15.0.1",
    "snowpack": "1.7.1",
    "standard": "14.3.4"
  },
  "dependencies": {
    "es-module-shims": "0.4.6",
    "rxjs": "6.5.5",
    "uce": "0.10.2"
  },
  "snowpack": {
    "webDependencies": [
      "es-module-shims",
      "rxjs",
      "rxjs/ajax",
      "rxjs/operators",
      "uce"
    ]
  }
}
